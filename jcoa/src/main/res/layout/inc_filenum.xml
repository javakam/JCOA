<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/gw_item_height_50"
        android:layout_marginTop="@dimen/one"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/padding_small_15"
        android:paddingRight="@dimen/padding_small_15">

        <TextView
            style="@style/GWTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="公文正件" />

        <EditText
            android:id="@+id/edtZhengJian"
            style="@style/GWTextView"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginLeft="@dimen/margin_lsmall_10"
            android:background="@mipmap/fen"
            android:inputType="number"
            android:maxLength="3"
            android:maxLines="1"
            android:paddingLeft="@dimen/padding_llsmall_5"
            android:paddingRight="@dimen/padding_llsmall_5"
            android:textCursorDrawable="@drawable/text_cursor" />

        <TextView
            style="@style/GWTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_llsmall_5"
            android:text="份" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/gw_item_height_50"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/padding_small_15"
        android:paddingRight="@dimen/padding_small_15">

        <TextView
            style="@style/GWTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="附        件" />

        <EditText
            android:id="@+id/edtFuJian"
            style="@style/GWTextView"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginLeft="@dimen/margin_lsmall_10"
            android:background="@mipmap/fen"
            android:inputType="number"
            android:maxLength="2"
            android:maxLines="1"
            android:paddingLeft="@dimen/padding_llsmall_5"
            android:paddingRight="@dimen/padding_llsmall_5"
            android:textCursorDrawable="@drawable/text_cursor" />

        <TextView
            style="@style/GWTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_llsmall_5"
            android:text="份" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/gw_item_height_50"
        android:background="@color/white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/padding_small_15"
        android:paddingRight="@dimen/padding_small_15">

        <TextView
            style="@style/GWTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="复   印   件" />

        <EditText
            android:id="@+id/edtFuYinJian"
            style="@style/GWTextView"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:layout_marginLeft="@dimen/margin_lsmall_10"
            android:background="@mipmap/fen"
            android:inputType="number"
            android:maxLength="2"
            android:maxLines="1"
            android:paddingLeft="@dimen/padding_llsmall_5"
            android:paddingRight="@dimen/padding_llsmall_5"
            android:textCursorDrawable="@drawable/text_cursor" />

        <TextView
            style="@style/GWTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_llsmall_5"
            android:text="份" />
    </LinearLayout>
</merge>